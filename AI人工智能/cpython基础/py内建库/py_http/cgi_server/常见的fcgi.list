#
# 1.proxy 网关
#
lighttpd - fast webserver with minimal memory footprint

libnginx-mod-http-cache-purge - Purge content from Nginx caches
nginx-extras - nginx web/proxy server (extended version)
nginx-full - nginx web/proxy server (standard version)
nginx-light - nginx web/proxy server (basic version)

apache2-utils - Apache HTTP Server (utility programs for web servers)
libapache2-mod-fcgid - FastCGI interface module for Apache 2
libapache2-mod-mapcache - tile caching server - Apache module
# apache2 for scgi (python 特有的)
libapache2-mod-scgi - Apache module implementing the SCGI protocol





#
# 2.常见语言的fastcgi
#
nodejs:
	node-fastcgi - FastCGI server for Node.js
	node-fastcgi-stream - Node.js module to read and write FastCGI records

lua:
	lua-wsapi - Web server API abstraction layer for the Lua language
	lua-wsapi-fcgi - Web server API fastcgi backend
	lua-wsapi-fcgi-dev - wsapi fastcgi development files for the Lua language

c/c++:
	spawn-fcgi - FastCGI process spawner
	libcgicc-dev - C++ class library for writing CGI applications (development files)
	libcgicc-doc - C++ class library for writing CGI applications (documentation)
	libcgicc3 - C++ class library for writing CGI applications (library)
	libfcgi-bin - FastCGI bridge from CGI
	libfcgi-dev - header files of FastCGI
	libfcgi0ldbl - shared library of FastCGI

php:
	php-cgi - server-side, HTML-embedded scripting language (CGI binary) (default)

python:
	python-flup - Implements Python Web Server Gateway Interface (WSGI)
	python-fcgi - Simple FastCGI module for Python
	python-scgi - Server-side implementation of the SCGI protocol





